{
    "name": "fbclit/opentextapi-laravel",
    "description": "An OpenText API service provider for Laravel",
    "type": "library",
    "keywords": [
        "api",
        "api-client",
        "opentext",
        "conent server"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Steve Bauman",
            "email": "steven_bauman@outlook.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "fbclit/opentextapi": "^0.0.3",
        "illuminate/support": "^6.0|^7.0"
    },
    "require-dev": {
        "mockery/mockery" : "^1.0",
        "phpunit/phpunit": "^7.0|^8.0",
        "orchestra/testbench": "^4.0|^5.0"
    },
    "archive": {
        "exclude": ["/tests"]
    },
    "autoload": {
        "psr-4": {
            "Fbcl\\OpenTextApi\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fbcl\\OpenTextApi\\Laravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fbcl\\OpenTextApi\\Laravel\\OpenTextServiceProvider"
            ]
        }
    }
}
